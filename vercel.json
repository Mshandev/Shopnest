{
    "functions": {
        "myFunction": {
          "maxDuration": 120,
          "memory": 512,
          "runtime": "@vercel/nodejs12"
        }
      },
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "build/**",
        "use": "@vercel/static"
      }
    ],
  
    "rewrites": [{ "source": "/(.*)", "destination": "index.js" }]
  }